<?xml version="1.0" encoding="utf-8"?>
<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
	<PropertyGroup>
		<ExecutablePath>$(MSBuildThisFileDirectory)bin\x64\;$(ExecutablePath)</ExecutablePath>
	</PropertyGroup>
	<ItemDefinitionGroup>
		<GLSL>
			<VariableName>%(Filename)%(Extension)</VariableName>
			<TargetFile>$(IntDir)%(Filename)%(Extension).cpp</TargetFile>
			<ObjectFileName>$(IntDir)%(Filename)%(Extension).obj</ObjectFileName>
		</GLSL>
	</ItemDefinitionGroup>
</Project>
